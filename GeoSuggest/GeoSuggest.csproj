<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
        <Version>1.0.0.0</Version>
        <AssemblyName>Yandex.API.GeoSuggest</AssemblyName>
        <PackageId>Yandex.API.GeoSuggest</PackageId>
        <RootNamespace>Yandex.API</RootNamespace>
        <Description>Implementation of yandex geosuggest api.</Description>
        <AutoIncrementPackageRevision>true</AutoIncrementPackageRevision>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageProjectUrl>
            https://github.com/risinghero/Yandex.API.GeoSuggest
        </PackageProjectUrl>
        <RepositoryUrl>https://github.com/risinghero/Yandex.API.GeoSuggest</RepositoryUrl>
        <Authors>RisingHeroMan</Authors>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\README.md" Pack="true" PackagePath="\" />
        <None Include="..\LICENSE" Pack="true" PackagePath="\" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    </ItemGroup>
</Project>
